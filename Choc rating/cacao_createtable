CREATE TABLE cacao 
( id_cacao serial PRIMARY KEY ,
 Company VARCHAR(50) NOT NULL,
 Specific_Bean_Origin VARCHAR(50)   ,
 Ref_review VARCHAR(10)   ,
 ReviewDate INT,
 Cocoa_Percent decimal(5, 2)   ,
 Company_Location VARCHAR(50)   ,
 Rating FLOAT ,
 Bean_Type VARCHAR(50)   ,
 Broad_Bean_Origin VARCHAR(50)
);



